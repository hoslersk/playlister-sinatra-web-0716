
<form action="/songs" method="post">
  <input type="text" name="song[name]" id="Name">
  <input id="Artist Name" type="text" name="artist[name]">
  <% Genre.all.each do |genre| %>
  <input id="<%= genre.name %>" type="checkbox" name="genre_ids[]" value="<%= genre.id %>"><%= genre.name %></input>
  <% end %>
  <input id="Create" type="submit" name="submit" value="Create">
</form>
